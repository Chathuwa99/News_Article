<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="686.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.test1.Controller.UserArticleController">
    <children>
        <VBox alignment="TOP_CENTER" layoutY="148.0" prefHeight="450.0" prefWidth="850.0" spacing="10">
            <TableView fx:id="articleTable" prefHeight="546.0" prefWidth="850.0">
                <columns>
                    <TableColumn fx:id="RecIdColumn" prefWidth="52.8" text="ID" />
                    <TableColumn fx:id="RecNameColumn" prefWidth="283.9" text="Article Name" />
                    <TableColumn fx:id="RecContentColumn" prefWidth="507.9" text="Content" />
                </columns>
            </TableView>
        </VBox>
        <Button fx:id="backButton" layoutX="30.0" layoutY="32.0" mnemonicParsing="false" onAction="#NavigateToArticlePageFromRecPage" text="Back" />

        <Label layoutX="115.0" layoutY="45.0" text="RECOMMENDED ARTICLE">
            <font>
                <Font name="Constantia Bold" size="49.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
